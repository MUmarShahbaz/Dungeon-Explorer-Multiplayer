[sub_resource type="GDScript" id="GDScript_tt30a"]
script/source = "extends CanvasLayer

@onready var main_panel = $Menu/Main
@onready var gamepad_layout = $Menu/Layouts/Gamepad
@onready var keyboard_layout = $Menu/Layouts/Keyboard
@onready var mode_panel = $Menu/Mode
@onready var multiplayer_panel = $Menu/Multiplayer
@onready var connect_panel = $Menu/Connect
@onready var address_bar =  $Menu/Connect/Address

enum menu_state {main, layout_gamepad, layout_keyboard, mode, multiplayer_mode, connect_to_host}
var current_state : menu_state = menu_state.main

func _ready() -> void:
	show_only(main_panel, $Menu/Main/Play)
	if OS.has_feature(\"web\"): $Menu/Mode/Multi.disabled = true

func _process(_delta: float) -> void:
	if Input.is_action_just_pressed(\"interact\"): roll_back()
	if Input.is_joy_known(0) and current_state == menu_state.layout_keyboard: update_state(menu_state.layout_gamepad)
	if !Input.is_joy_known(0) and current_state == menu_state.layout_gamepad: update_state(menu_state.layout_keyboard)

func roll_back():
	match current_state:
		menu_state.main:
			return
		menu_state.layout_gamepad:
			show_only(main_panel, $Menu/Main/Play)
			current_state = menu_state.main
		menu_state.layout_keyboard:
			show_only(main_panel, $Menu/Main/Play)
			current_state = menu_state.main
		menu_state.mode:
			show_only(main_panel, $Menu/Main/Play)
			current_state = menu_state.main
		menu_state.multiplayer_mode:
			show_only(mode_panel, $Menu/Mode/Single)
			current_state = menu_state.mode
		menu_state.connect_to_host:
			show_only(multiplayer_panel, $Menu/Multiplayer/Host)
			current_state = menu_state.multiplayer_mode

func update_state(new_state):
	match new_state:
		menu_state.main:
			return
		menu_state.layout_gamepad:
			show_only(gamepad_layout, $Menu/Back)
			current_state = menu_state.layout_gamepad
		menu_state.layout_keyboard:
			show_only(keyboard_layout, $Menu/Back)
			current_state = menu_state.layout_keyboard
		menu_state.mode:
			show_only(mode_panel, $Menu/Mode/Single)
			current_state = menu_state.mode
		menu_state.multiplayer_mode:
			show_only(multiplayer_panel, $Menu/Multiplayer/Host)
			current_state = menu_state.multiplayer_mode
		menu_state.connect_to_host:
			show_only(connect_panel, address_bar)
			current_state = menu_state.connect_to_host


func show_only(element_to_show, element_to_focus = null):
	main_panel.hide()
	gamepad_layout.hide()
	keyboard_layout.hide()
	mode_panel.hide()
	multiplayer_panel.hide()
	connect_panel.hide()
	element_to_show.show()
	if element_to_focus: element_to_focus.grab_focus()


func _on_play_pressed() -> void:
	update_state(menu_state.mode)


func _on_controls_pressed() -> void:
	update_state(menu_state.layout_gamepad)


func _on_single_pressed() -> void:
	await $Fade.fade_out()
	get_tree().change_scene_to_file(\"res://SinglePlayerMode.tscn\")


func _on_multi_pressed() -> void:
	update_state(menu_state.multiplayer_mode)


func _on_host_pressed() -> void:
	var multiplayer_mode = load(\"res://MultiPlayerMode.tscn\").instantiate()
	multiplayer_mode.mode = &\"HOST\"
	await $Fade.fade_out()
	get_tree().change_scene_to_node(multiplayer_mode)


func _on_join_pressed() -> void:
	update_state(menu_state.connect_to_host)


func _on_connect_pressed() -> void:
	var multiplayer_mode = load(\"res://MultiPlayerMode.tscn\").instantiate()
	multiplayer_mode.mode = address_bar.text
	await $Fade.fade_out()
	get_tree().change_scene_to_node(multiplayer_mode)


func _on_back_pressed() -> void:
	roll_back()
"

